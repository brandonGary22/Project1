<div class="jumbotron">
      <div class="container">
        <h1 class="display-3">Forgot your password did ya now?</h1>
        <p>Its ok, enter your email and well let you change your password. Do better next time</P>
        </div>
    </div>

<%= form_for(resource, as: resource_name, url: password_path(resource_name), html: { method: :post }) do |f| %>
  <%= render "devise/shared/error_messages", resource: resource %>

  <div class="field">
   <label for="exampleInputEmail1">Email address</label>
   <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter email"<%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>

  <div class="actions">
  <button type="submit" class="btn btn-primary">Send me reset password instructions</button>
  </div>
<% end %>
<p></p>
<%= render "devise/shared/links" %>


<nav class="navbar navbar-expand-md navbar-dark fixed-top bg-dark">
      <a class="navbar-brand" href="#">Brandons Dope App</a>
      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarsExampleDefault" aria-controls="navbarsExampleDefault" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>

      <div class="collapse navbar-collapse" id="navbarsExampleDefault">
        <ul class="navbar-nav mr-auto">
          <li class="nav-item active">
          <a class = "nav-link" <%= link_to 'Home', projects_path %> </a>
          </li>
        </ul>
      

      <div class="collapse navbar-collapse" id="navbarsExampleDefault">
       <ul class="navbar-nav mr-auto">
         <li class="nav-item active">
           <% if user_signed_in? %>
             <a class = "nav-link" <%= link_to 'Edit profile', edit_user_registration_path, :class => 'navbar-link' %></a> 
             <li>
             <a class = "nav-link" <%= link_to "Logout", destroy_user_session_path, method: :delete, :class => 'navbar-link'  %></a>
             <li>
           <% else %>
             <a class = "nav-link"<%= link_to "Sign up", new_user_registration_path, :class => 'navbar-link'  %> </a>
             <li>
             <a class = "nav-link" <%= link_to "Login", new_user_session_path, :class => 'navbar-link'  %> </a>
           <% end %>
         </li>
       </ul>
     </div>
   </nav>

<br>